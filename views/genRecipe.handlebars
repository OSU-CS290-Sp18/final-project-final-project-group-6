<!DOCTYPE html>

<html>
  <head>

    <meta charset="utf-8">
    <title>Generated Recipes</title>

    <link rel="stylesheet" href="genRecipe.css" media="screen">

  </head>

  <body>

    <header>

      <h1 class="site-title">Generated Recipes</h1>

      <nav class="navbar">
        <ul class="navlist">
          <li class="navitem ingredient">
            <button type="button" class="ingredient-x-button">X</button>IngredientName
          </li>
          <li class="navitem navbar-search">
            <input type="text" id="navbar-search-input" placeholder="Add Ingredient">
            <button type="button" id="navbar-search-button"><i class="fas fa-search"></i></button>
          </li>
        </ul>
      </nav>
    </header>

    <main class="recipe-container">

      {{#each recipes}}
        {{> recipe}}
      {{/each}}

    </main>

  </body>

</html>







</html>
